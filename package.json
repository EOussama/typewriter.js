{
  "name": "typewriterjs",
  "version": "4.0.0",
  "description": "A typewriter for the web.",
  "main": "./src/typewriter.js",
  "directories": {
    "doc": "docs",
    "dist": "dist"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npx gulp",
    "docs": "rm -rf \"docs/assets/js/lib\" && mkdir \"docs/assets/js/lib\" && cp \"./dist/typewriter.js\" \"./docs/assets/js/lib/\"",
    "prod": "npm run clean && npm run build && npm run docs",
    "test": "npx jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EOussama/typewriterjs.git"
  },
  "keywords": [
    "animation",
    "typewriter",
    "js",
    "library"
  ],
  "author": "EOussama",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EOussama/typewriterjs/issues"
  },
  "homepage": "https://github.com/EOussama/typewriterjs#readme",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-beautify": "^2.0.1",
    "gulp-cli": "^2.0.1",
    "gulp-rename": "^1.4.0",
    "gulp-uglify": "^3.0.1",
    "jest": "^23.6.0"
  }
}
